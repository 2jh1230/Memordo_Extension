{
    "manifest_version": 3,
    "name": "memordo Extension",
    "version": "1.0",
    "description": "This is a memordo extension.",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "storage",
      "unlimitedStorage"
    ]
  }
  
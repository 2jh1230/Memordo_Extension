{
  "manifest_version": 3,
  "name": "memordo Extension",
  "version": "1.0",
  "description": "This is a memordo extension.",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "identity"
  ],
  "oauth2": {
    "client_id": "395176768661-1ad6e48ul9h5a4or7s3sqju9b53vn49q.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "host_permissions": [
    "https://www.googleapis.com/"
  ]
}
